@model string
@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>Getting started with JSON Form</title>
    <link rel="stylesheet" style="text/css" href="../../../node_modules/json-form/deps/opt/bootstrap.css" />
</head>
<body>
    <h1>Getting started with JSON Form</h1>
    <form></form>

    <script type="text/javascript" src="../../../node_modules/json-form/deps/jquery.min.js"></script>
    <script type="text/javascript" src="../../../node_modules/json-form/deps/underscore.js"></script>
    <script type="text/javascript" src="../../../node_modules/json-form/deps/opt/jsv.js"></script>
    <script type="text/javascript" src="../../../node_modules/json-form/lib/jsonform.js"></script>

    <script type="text/javascript">
        $('form').jsonForm(@Html.Raw(Model));
    </script>
</body>
</html>